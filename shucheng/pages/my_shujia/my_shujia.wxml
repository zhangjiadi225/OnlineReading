<view class="conent">
  <view
    class="conent-item"
    wx:for="{{dataList}}"
    wx:key="*this"
    bindtap="goDetail"
    data-obj="{{item}}"
  >
    <view class="img">
      <image src="{{item.img}}" />
    </view>
    <view class="item-title">{{item.title}}</view>
    <view
      class="del"
      data-item="{{item}}"
      catchtap="del"
    >删除
    </view>
  </view>
</view>

